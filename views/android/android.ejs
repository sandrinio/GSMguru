<% include ../partials/header %>
<body class="hold-transition skin-purple sidebar-collapse sidebar-mini">
<div class="wrapper">

    <!-- Main Header -->
    <header class="main-header">

        <!-- Logo -->
        <a href="/" class="logo">
            <!-- mini logo for sidebar mini 50x50 pixels -->
            <span class="logo-mini"><b>G</b>urus</span>
            <!-- logo for regular state and mobile devices -->
            <span class="logo-lg"><b>GSM </b>Gurus</span>
        </a>

        <!-- Header Navbar -->
        <nav class="navbar navbar-static-top" role="navigation">
            <!-- Sidebar toggle button-->
            <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                <span class="sr-only">Toggle navigation</span>
            </a>
            <!-- Navbar Right Menu -->

        </nav>
    </header>

    <!-- Left side column. contains the logo and sidebar -->
    <aside class="main-sidebar">

        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
            <!-- Sidebar Menu -->
            <ul class="sidebar-menu">
                <li class="header">Navigation Bar</li>
                <!-- Optionally, you can add icons to the links -->
                <li class="active"><a href="#"><i class="fa fa-home" id="sidebar-icon" aria-hidden="true"></i> <span>საწყისი გვერდი</span></a></li>
                <li><a href="#"><i class="fa fa-newspaper-o" id="sidebar-icon-news" aria-hidden="true"></i><span>სიახლეები</span></a></li>
                <li class="treeview">
                    <a href="#"><i class="fa fa-android" id="sidebar-icon" aria-hidden="true"></i> <span>Android</span>
                        <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i></span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="#">ინსტრუქციები</a></li>
                        <li><a href="#">აპლიკაციები</a></li>
                        <li><a href="#">სიახლე სისტემაზე</a></li>
                    </ul>
                </li>


                <li class="treeview">
                    <a href="#"><i class="fa fa-apple" id="sidebar-icon" aria-hidden="true"></i> <span>iPhone</span>
                        <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i></span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="#">ინსტრუქციები</a></li>
                        <li><a href="#">აპლიკაციები</a></li>
                        <li><a href="#">სიახლე სისტემაზე</a></li>
                    </ul>
                </li>
            </ul>
            <!-- /.sidebar-menu -->
        </section>
        <!-- /.sidebar -->
    </aside>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">

        </section>

        <!-- Main content -->
        <section class="content">

            <!-- =============================================  Your Page Content Here  ============================================== -->

            <div class="row">
                <!--main information -->
                <div class="col-md-12">
                    <div class="box">
                        <h3 class="text-center main-info-header">ისწავლე სმარტფონი ჩვენთან ერთად <br><span id="loadLater">ვიდეო </span><span id="showLater2">ინსტრუქციებით</span> </h3>
                        <div class="div-center">
                            <div class="inline-block">
                                <p class="text-center">აუცილებელი ინფო</p>
                                <a href="#"><span class="info-box-icon-os-edges bg-red-gradient"><i class="fa fa-exclamation" aria-hidden="true"></i></span></a>
                            </div>
                            <div class="inline-block">
                                <p class="text-center">ყველაფერი ანდროიდზე</p>
                                <a href="/smartphones/android"><span class="info-box-icon-os bg-green-gradient"><i class="fa fa-android" aria-hidden="true"></i></span></a>
                            </div>
                            <div class="inline-block">
                                <p class="text-center">ყველაფერი აიფოუნზე</p>
                                <a href="#"><span class="info-box-icon-os bg-black-gradient"><i class="fa fa-apple" aria-hidden="true"></i></span></a>
                            </div>
                            <div class="inline-block info-box-last">
                                <p class="text-center">აპების მიმოხილვა</p>
                                <a href="#"><span class="info-box-icon-os-edges bg-yellow-gradient"><i class="fa fa-gamepad" aria-hidden="true"></i></span></a>


                            </div>
                        </div>
                    </div>
                </div>
            </div> <!-- END main information -->




            <!--All Feeds row-->
            <div class="row">
                <!--Blog feed-->
                <div class="col-md-8">
                    <div class="box box-purple">
                        <div class="box-header with-border">
                            <h3 class="box-title">სიახლეები</h3>
                        </div>
                        <div class="box-body">
                            <ul class="products-list product-list-in-box">
                                <% var x = 5 %>
                                <% for(i = 0; i < x; i++) { %>
                                <li class="item">
                                    <div class="blog-image">
                                        <img src="../dist/static_images/placeholder.jpg" alt="">
                                    </div>
                                    <div class="blog-info">
                                        <a href="#" class="blog-title">ახალი ამბავი</a>
                                        <span class="blog-description">
                                            Suspendisse sapien ante, auctor congue maximus vel, fermentum nec eros. Maecenas
                                            at tellus non justo consequat suscipit. Nulla cursus est vel nibh vestibulum, rhoncus
                                            Praesent maximus in velit ac congue. Morbi non dictum
                                        </span>
                                    </div>
                                    <a href="#" class="greenBtn">მეტი</a>
                                </li>
                                <% } %>
                            </ul>
                            <div class="box-footer text-center">
                                <!--             PAGINATION             -->
                                <nav aria-label="Page navigation">
                                    <ul class="pagination">
                                        <li>
                                            <a href="#" aria-label="Previous">
                                                <span aria-hidden="true">&laquo;</span>
                                            </a>
                                        </li>
                                        <li><a href="#">1</a></li>
                                        <li><a href="#">2</a></li>
                                        <li><a href="#">3</a></li>
                                        <li><a href="#">4</a></li>
                                        <li><a href="#">5</a></li>
                                        <li>
                                            <a href="#" aria-label="Next">
                                                <span aria-hidden="true">&raquo;</span>
                                            </a>
                                        </li>
                                    </ul>
                                </nav><!--PAGINATION END-->
                            </div>
                        </div>
                    </div>
                </div>
                <!--END Blog Feed-->

                <!--Sidebar-->
                <div class="col-md-4">
                    <div class="box box-purple">
                        <div class="box-header with-border">
                            <p class="box-title">აუცილებლად უნდა ვიცოდეთ<p>
                        </div>
                        <div class="box-body">
                            <ul class="products-list product-list-in-box">
                                <% var x = 5 %>
                                <% for(i = 0; i < x; i++){ %>
                                <li class="item">
                                    <div class="product-img">
                                        <img src="/dist/static_images/default-50x50.gif">
                                    </div>
                                    <div class="product-info">
                                        <a href="#" class="product-title">დავრეგისტრირდეთ Play Market-ზე</a>
                                        <span class="product-description">
                                            თავად ტკივილი არავის უყვარს, არავინ ეძებს მას და არავის სურს იგი, უბრალოდ იმიტომ, რომ ის ტკივილია...
                                        </span>
                                    </div>
                                </li>
                                <% } %>
                            </ul>
                        </div>

                    </div>
                    <div class="box box-purple">
                        <div class="box-header with-border">
                            <p class="box-title">TOP 5 Apps</p>
                        </div>
                        <div class="box-body">
                            <ul class="products-list product-list-in-box">
                                <% var x = 5 %>
                                <% for(i = 0; i < x; i++){ %>
                                <li class="item">
                                    <div class="app-img">
                                        <img src="/dist/static_images/default-50x50.gif">
                                    </div>
                                    <div class="app-title">
                                        <a href="#">Viber 6.6.0.888 (120251)</a>
                                        <small class="app-date">Date: <em>01.03.2017</em></small>
                                    </div>
                                </li>
                                <% } %>
                            </ul>
                        </div>
                        <div class="box-footer text-center">
                            <a href="#">ყველა</a>
                        </div>

                    </div>

                </div>
                <!--Sidebar-->
            </div>
            <!--END All Feeds row-->

        </section>

        <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->


<% include ../partials/footer %>